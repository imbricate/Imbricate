{
  "name": "@imbricate/core",
  "packageManager": "yarn@4.1.1",
  "version": "1.13.0",
  "description": "Core for Imbricate",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
