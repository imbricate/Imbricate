    "@sudoo/marked-mixin-json": "^1.0.0",
    "@sudoo/marked-mixin-math": "^1.4.0",
    "@sudoo/marked-mixin-object": "^1.3.0",
    "@sudoo/marked-mixin-parse": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@babel/preset-typescript": "^7",
